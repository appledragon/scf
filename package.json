{
  "name": "scf",
  "displayName": "search current file",
  "description": "search current file",
  "publisher": "CuiLiAn",
  "version": "0.0.4",
  "icon": "assets/logo.png",
  "engines": {
    "vscode": "^1.40.0"
  },
  "keywords": ["vscode", "search", "current", "file"],
  "homepage": "https://github.com/appledragon/scf",
  "repository": {
      "type": "git",
      "url": "https://github.com/appledragon/scf"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.scf"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.scf",
        "title": "Search Current File"
      }
    ],
    "menus": {
      "editor/title/context": [
        {
          "when": "",
          "command": "extension.scf",
          "group": "navigation@0"
        }
      ],
      "editor/context": [
        {
          "when": "",
          "command": "extension.scf",
          "group": "navigation@0"
        }
      ]
    },
    "keybindings": [
      {
        "command": "extension.scf",
        "key": "alt+s",
        "mac": "alt+s",
        "when": "editorTextFocus"
      }]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "√è": {
    "@types/vscode": "^1.69.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  },
  "dependencies": {
    "module-name": "^0.0.1-security"
  }
}
